def foo1(str:String) = match str {
    [x]++x2 => x
    [] => 'y'
}

/*
utest foo1("foo")       'f'
utest foo1("k")         'k'
utest foo1("")          'y'
*/

def map(f:a->b, lst:[a]) -> [b] = match lst {
    [x]++xs => [f] ++ map(f,xs)
    [] => []
}
